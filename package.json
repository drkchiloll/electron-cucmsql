{
  "name": "cucm-sequelizer",
  "version": "1.0.0",
  "description": "CUCM SQL Query Tool",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "clean": "cd dist && rm app.bundle.js app.bundle.js.map main.bundle.js main.bundle.js.map && cd ..",
    "build:dev": "webpack --mode development --watch --progress --profile",
    "build:dist": "node_modules/.bin/webpack --mode production --progress --colors && rm dist/app.bundle.js.map && rm dist/main.bundle.js.map",
    "start": "electron .",
    "package:macos": "node_modules/.bin/electron-packager ./ CUCMSequelizer --platform=darwin --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.4 --icon ./resources/sequelizer.icns",
    "package:win64": "electron-packager ./ CUCMSequelizer --platform=win32 --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.2 --icon ./resources/sequelizer.ico"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bluebird": "^3.5.2",
    "@types/electron": "^1.4.33",
    "@types/fixed-data-table": "^0.6.31",
    "@types/jquery": "^2.0.41",
    "@types/material-ui": "^0.17.4",
    "@types/nedb": "^1.8.2",
    "@types/node": "^7.0.55",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/request": "^2.47.0",
    "@types/xmldom": "^0.1.29",
    "awesome-typescript-loader": "^5.0.0-1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.0",
    "css-loader": "^0.26.1",
    "electron": "^1.8.2",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.5.7",
    "file-loader": "^0.10.1",
    "fixed-data-table-2": "^0.7.15",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.20.0",
    "moment": "^2.18.1",
    "nedb-core": "^3.0.6",
    "react": "^16.2.0",
    "react-ace": "^4.4.0",
    "react-csv-creator": "^1.0.2",
    "react-dom": "^16.2.0",
    "request": "^2.79.0",
    "style-loader": "^0.13.1",
    "typescript": "^2.3.4",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.5.6",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-merge": "^3.0.0",
    "xmldom": "^0.1.27"
  }
}
